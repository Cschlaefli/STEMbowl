@model Question

<br /> <br />

@using(Html.BeginForm())
{

    @Html.AntiForgeryToken()

    @Html.EditorForModel()

    <div class="col-md-offset-1 col-md-10">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>

}
@section Scripts
{
    <script>
    $(document).ready(function() {
        $('#Format').on('change', function () {
           $('#MultipleChoice').toggle(this.value == 'MultipleChoice');
           $('#ShortAnswer').toggle(this.value == 'ShortAnswer');
           $('#TrueFalseDiv').toggle(this.value == 'TrueFalse');
        });
    });
    console.log('test');
    </script>
}