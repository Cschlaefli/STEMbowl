@page
@model TeamManagmentModel
@{
    ViewData["Title"] = "Team Managment";
    ViewData["ActivePage"] = ManageNavPages.TeamManagment;
}
<hr />
<h4>@ViewData["Title"]</h4>
<partial name="_StatusMessage" for="StatusMessage" />
<div class="row">
    <div clas="col-md-3">
        <partial name="_ManageNav" />
    </div>
    <div class="col-md-6">
        <div class="row">
        You are already part of team @Model.Display.TeamName, 
        led by @Model.Display.TeamLead,
        contact them to add members.
        </div>
    <form class="row" asp-page-handler="quit" method="post">
        <button class="btn btn-danger">Quit Team</button>
    </form>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}